<p id="notice"><%= notice %></p>

<h1>Alunos</h1>

<table class="table">
  <thead class="table-inverse">
    <tr>
      <th>Nome</th>
      <th>Idade</th>
      <th>Nacionalidade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alunos.each do |aluno| %>
      <tr>
        <td><%= aluno.nome %></td>
        <td><%= aluno.idade %></td>
        <td><%= aluno.nacionalidade %></td>
      </tr>
    <% end %>
  </tbody>
</table>
